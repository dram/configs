!! XFT settings
Xft.dpi:	98
Xft.antialias:	true
Xft.rgba:	rgb
Xft.hinting:	true
Xft.autohint:	true
Xft.hintstyle:	hintslight
Xft.lcdfilter:	lcddefault

!! Terminal colors
! Black
URxvt.color0: #000000
URxvt.color8: #555555
! Red
URxvt.color1: #CC0000
URxvt.color9: #FF5555
! Green
URxvt.color2: #00CC00
URxvt.color10: #55FF55
! Yellow
URxvt.color3: #CC6600
URxvt.color11: #FFFF55
! Blue
URxvt.color4: #0000CC
URxvt.color12: #5555FF
! Magenta
URxvt.color5: #CC00CC
URxvt.color13: #FF55FF
! Cyan
URxvt.color6: #00CCCC
URxvt.color14: #55FFFF
! White
URxvt.color7: #CCCCCC
URxvt.color15: #FFFFFF

!! rxvt-unicode settings
URxvt.geometry: 80x33
URxvt.font: xft:Liberation Mono:pixelsize=14
URxvt.scrollBar: false
URxvt.perl-ext-common: default,tabbedex,clipboard
URxvt.tabbed.new-button: no
URxvt.tabbed.tabbar-fg: 8
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    3
URxvt.tabbed.tab-bg:    0
URxvt.preeditType: Root

URxvt.letterSpace: -1

URxvt.foreground: #BBBBBB
URxvt.background: #000000
